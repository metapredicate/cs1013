class Histogram
{
  private int x, y, width, height, lowValue, highValue, increment;
  Histogram(int x, int y, int width, int height, int lowValue, int highValue, int increment)
  {
    this.x = x;
    this.y = y;
    this.width = width;
    this.height = height;
    this.lowValue = lowValue;
    this.highValue = highValue;
    this.increment = increment;
  }
}